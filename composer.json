{
    "name": "s1k3/livewire-v4-patch",
    "description": "This Package provides comprehensive tools for converting traditional Livewire class components to Model-Focused Components (MFC) in Livewire v4",
    "type": "library",
    "minimum-stability": "dev",
    "license": "MIT",
    "keywords": [
        "livewire",
        "v4",
        "patch"
    ],
    "authors": [
        {
            "name": "Robin Khan",
            "email": "sk118420@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "LivewireV4\\": "src/"
        }
    },
    "require": {
        "php": "^8.2",
        "nikic/php-parser": "^5.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "LivewireV4\\LivewireV4PatchServiceProvider"
            ]
        }
    },
    "require-dev": {
        "pestphp/pest-plugin-laravel": ">=4.0"
    }
}